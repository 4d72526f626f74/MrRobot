local setmetatable = setmetatable
local trans = {}

local T = setmetatable(trans, {
    __index = function(self, key)
        if not rawget(self, key) then
            return key
        end

        return rawget(self, key)
    end,
    __mode = 'k',
    __call = function(self, key)
        return self[key]
    end
})

return T