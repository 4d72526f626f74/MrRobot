    [229] = 'Acquiring goods for a Hangar',
    [230] = 'Selling goods from a Hangar',
    [237] = 'Taking part in Nightclub business',
    [243] = 'Working for Ms. Baker',
    [271] = 'Conducting Auto Shop business'
}

function Shared:GetPlayerMissionState(pid)
    local g = self.ScriptGlobal(1853988 + 1 + (pid * 867))
    local index = self.ReadInt(g)

    return self.MissionStates[index] or 'Unknown'
end

